[Unit]
Description=nsqlookupd
After=network.target

[Service]
ExecStart=/bin/sh -c 'exec <%= scope.lookupvar('nsq::bin_dir')%>/bin/nsqlookupd -config <%= scope.lookupvar('nsq::conf_dir') %>/nsqlookupd.conf'
Type=simple
PIDFile=/var/run/nsqlookupd.pid
LimitNOFILE=65536

[Install]
WantedBy=default.target
